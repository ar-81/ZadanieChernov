procedure Test1;
  var TFMain : OleVariant;
  var MDIClient : OleVariant;
  var tcxDBTreeList : OleVariant;
  var tcxCustomInnerTextEdit : OleVariant;
begin
  TFMain := Aliases.TestEnvironment_TD32_.FMain;
  TFMain.dxRibbon.ClickItemXY('dxRibbonTab1|[0]|Форма с TreeList', 51, 4);
  MDIClient := TFMain.MDIClient;
  tcxDBTreeList := MDIClient.FTreeList.tlTest;
  tcxDBTreeList.wChildView[1].Collapse(2);
  tcxDBTreeList.Collapse(1);
  tcxDBTreeList.Drag(1357, 233, 8, 118);
  tcxDBTreeList.wChildView[3].wChildView[2].ClickCell(1, 'name');
  tcxCustomInnerTextEdit := tcxDBTreeList.TcxMaskEdit.TcxCustomInnerTextEdit;
  tcxCustomInnerTextEdit.Click(29, 6);
  tcxCustomInnerTextEdit.SetText('Namerrrrr58');
  tcxDBTreeList.Click(455, 348);
  tcxDBTreeList.Drag(1356, 354, 9, -246);
  tcxDBTreeList.Expand(1);
  tcxDBTreeList.wChildView[1].Expand(2);
  tcxDBTreeList.wChildView[1].wChildView[2].ClickCell(0, 'name');
  tcxCustomInnerTextEdit := tcxDBTreeList.TcxMaskEdit2.TcxCustomInnerTextEdit;
  tcxCustomInnerTextEdit.Click(34, 7);
  tcxCustomInnerTextEdit.Drag(42, 7, -44, 1);
  tcxCustomInnerTextEdit.SetText('');
  tcxCustomInnerTextEdit.Keys('[Del]');
  tcxDBTreeList.Click(243, 371);
  MDIClient.Click(147, 586);
end;
